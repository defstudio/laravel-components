{
    "name": "defstudio/laravel-components",
    "description": "A collection of laravel blade components",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Fabio Ivona",
            "email": "fabio.ivona@defstudio.it",
            "homepage": "https://defstudio.it",
            "role": "Developer"
        },
        {
            "name": "Daniele Romeo",
            "email": "danieleromeo@defstudio.it",
            "homepage": "https://defstudio.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "maatwebsite/excel": "^3.1",
        "tightenco/ziggy": "^1.0.0",
        "defstudio/supercharged-carbon": "^0.0.1"
    },
    "autoload": {
        "psr-4": {
            "DefStudio\\Components\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DefStudio\\Components\\ComponentServiceProvider"
            ]
        }
    }
}
